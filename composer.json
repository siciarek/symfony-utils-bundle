{
    "name": "siciarek/symfony-utils-bundle",
    "type": "library",
    "description": "Bunch of utilities for Symfony2",
    "keywords": [
        "cookie",
        "privacy policy",
        "symfony2",
        "utilities",
        "helpers",
        "bundle"
    ],
    "homepage": "http://github.com/siciarek/symfony-utils-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacek Siciarek",
            "email": "siciarek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "uran1980/fancybox-bundle": "*",
        "phpunit/phpunit": "3.7.*",
        "behat/behat": "*",
        "behat/mink": "*",

        "behat/symfony2-extension": "*",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "behat/mink-sahi-driver": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-zombie-driver": "*"
    },
    "minimum-stability": "dev",
    "require-dev": {
        
    },
    "autoload": {
        "psr-0": {
            "Siciarek\\SymfonyUtilsBundle": "src/"
        }
    },
    "bin": [
        "bin/behat/xbehat",
        "bin/selenium/selenium"
    ]
}
