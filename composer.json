{
    "name": "siciarek/symfony-utils-bundle",
    "type": "library",
    "description": "Bunch of utilities for Symfony2",
    "keywords": [
        "symfony2",
        "utilities",
        "helpers",
        "bundle"
    ],
    "homepage": "http://github.com/siciarek/symfony-utils-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacek Siciarek",
            "email": "siciarek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "minimum-stability": "dev",
    "require-dev": {

    },
    "autoload": {
        "psr-0": {
            "Siciarek\\SymfonyUtilsBundle": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/xbehat/behat.bat",
        "bin/xbehat/behat"
    ]
